{
  "name": "steamws-monitor",
  "version": "0.2.2",
  "description": "An application that monitors steam workshop files for updates",
  "repository": {
    "type": "git",
    "url": "https://github.com/dixon13/steamws-monitor.git"
  },
  "keywords": [
    "reactjs",
    "electron",
    "reactstrap",
    "steam",
    "steamworkshop",
    "workshop",
    "monitor"
  ],
  "author": "Dixon Begay",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dixon13/steamws-monitor/issues"
  },
  "homepage": "https://github.com/dixon13/steamws-monitor",
  "main": "./src/es6-init.js",
  "scripts": {
    "start": "rm -rf .cache && cross-env NODE_ENV=development electron .",
    "clean-cache": "rm -rf .cache",
    "compile": "cross-env NODE_ENV=production electron-compile --appDir . ./src",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "build"
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "devtron": "^1.4.0",
    "electron-builder": "^19.28.4",
    "electron-builder-squirrel-windows": "^19.37.0",
    "electron-compilers": "^5.9.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-prebuilt-compile": "1.6.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "npm-run-all": "^4.1.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "recursive-readdir": "^2.2.1",
    "redux-devtools": "^3.3.2"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "bootstrap": "^4.0.0-alpha.6",
    "cross-env": "^5.0.1",
    "electron-compile": "^6.4.1",
    "electron-context-menu": "^0.9.1",
    "electron-log": "^2.2.9",
    "electron-main-notification": "^1.0.1",
    "electron-settings": "^3.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^2.13.0",
    "rcedit": "^0.9.0",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-split-pane": "^0.1.65",
    "react-transition-group": "^1.2.0",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "rimraf": "^2.6.1"
  },
  "build": {
    "appId": "com.squirrel.steamws.SteamWS",
    "productName": "Steam Workshop Monitor",
    "asar": true,
    "copyright": "Copyright (C) 2017 Dixon Begay",
    "win": {
      "target": "squirrel",
      "icon": "./src/static/images/logos/favicon.ico",
      "publish": [
        "github"
      ]
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/dixon13/steamws-monitor/master/src/static/images/logos/favicon.ico",
      "loadingGif": "Loading.gif",
      "remoteReleases": true,
      "publish": [
        "github"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "dixon13",
        "repo": "steamws-monitor"
      }
    ]
  }
}
