{
  "name": "steamws-monitor",
  "version": "0.1.0",
  "description": "An application that monitors steam workshop files for updates",
  "repository": {
    "type": "git",
    "url": "https://github.com/dixon13/steamws-monitor.git"
  },
  "keywords": [
    "reactjs",
    "electron",
    "reactstrap",
    "steam",
    "steamworkshop",
    "workshop",
    "monitor"
  ],
  "author": "Dixon Begay",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dixon13/steamws-monitor/issues"
  },
  "homepage": "https://github.com/dixon13/steamws-monitor",
  "main": "src/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.24.1",
    "electron-devtools-installer": "^2.1.0",
    "electron-prebuilt-compile": "1.6.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "redux-devtools": "^3.3.2"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "babel-preset-stage-2": "^6.22.0",
    "bootstrap": "^4.0.0-alpha.6",
    "cross-env": "^3.2.4",
    "electron-compile": "^6.4.0",
    "electron-main-notification": "^1.0.1",
    "electron-settings": "^3.0.14",
    "electron-squirrel-startup": "^1.0.0",
    "rcedit": "^0.9.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-split-pane": "^0.1.61",
    "react-transition-group": "^1.1.3",
    "reactstrap": "^4.3.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "rimraf": "^2.6.1"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "dir": "./src",
        "appCopyright": "Copyright (C) 2017 Dixon Begay",
        "appVersion": "0.1.0",
        "electronVersion": "1.6.2",
        "asar": true,
        "icon": "./src/static/images/logos/favicon.ico",
        "name": "Steam Workshop Monitor",
        "win32metadata": {
          "CompanyName": "Dixon Begay",
          "FileDescription": "Application to monitor the Steam Workshop",
          "ProductName": "Steam Workshop Monitor"
        }
      },
      "electronWinstallerConfig": {
        "loadingGif": "./Loading.gif",
        "exe": "Steam Workshop Monitor.exe",
        "version": "0.1.0",
        "title": "Steam Workshop Monitor",
        "name": "Steam_Workshop_Monitor",
        "iconUrl": "https://raw.githubusercontent.com/dixon13/steamws-monitor/master/src/static/images/logos/favicon.ico"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "dixon13",
        "name": "steamws-monitor"
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
