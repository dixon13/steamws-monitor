{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["react", "es2016-node5"],
        "plugins": ["transform-async-to-generator"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["react", "es2016-node5"],
        "plugins": ["transform-async-to-generator"],
        "sourceMaps": false
      }
    }
  }
}
