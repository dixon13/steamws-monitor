{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["react", ["env", { "targets": { "electron": "1.6.2" } }], "stage-2"],
        "plugins": ["react-hot-loader/babel", "transform-decorators-legacy"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["react", ["env", { "targets": { "electron": "1.6.2" } }], "stage-2"],
        "plugins": ["transform-es2015-classes"],
        "sourceMaps": "none"
      }
    }
  }
}